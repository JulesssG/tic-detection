alias v='vim'
alias g='git'
alias gs='git status -s -b'
alias gac='git -a commit'
alias gcm='git commit -m'
alias ga='git add'
alias gc='git commit'
alias gd='git diff'
alias gb='git branch'
alias gpl='git pull'
alias gp='git push'
alias gl='git --no-pager log -10 --oneline'
alias gll='git log --oneline --graph'
